<article class="card mb-2">
    <div class="card-body">
        <p>{{origin}}</p>
        <p>{{shortURL}}</p>
        <a href="/delete/{{_id}}" class="btn btn-danger">Eliminar</a>
        <a href="/update/{{_id}}" class="btn btn-warning">Editar</a>
        <button data-short="{{shortURL}}" href="/copiar/{{_id}}" class="btn btn-info">Copiar</button>
    </div>
</article>